{
	"name": "Nesine.com Tam Ekran",
	"version": "1.2",
	"description": "Nesine.com sitesinde yayınlanan canlı maç yayınlarını tam ekran yapmanıza olanak sağlar.",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"64": "icons/icon64.png",
		"72": "icons/icon72.png",
		"96": "icons/icon96.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": "icons/icon64.png"
    },
	"permissions": [
		"*://*.nesine.com/*",
		"webRequest",
		"webRequestBlocking"
	],
	"content_scripts": [
		{
			"matches": [
				"*://*.nesine.com/LiveBroadCast/Player*",
				"*://*.nesine.com/iddaa/canli-iddaa-canli-bahis*"
			],
			"run_at": "document_end",
			"js": ["script.js"]
		},
		{
			"matches": [
				"*://*.nesine.com/iddaa/canli-iddaa-canli-bahis*"
			],
			"run_at": "document_end",
			"js": ["jquery-3.6.0.slim.min.js", "script2.js"]
		}
	]
}